{
  "extends": ["airbnb/base", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "max-len": ["error", 120],
    "max-depth": ["error", 4],
    "import/prefer-default-export": ["off"],
    "import/no-default-export": ["error"],
    "import/extensions": [
      "error",
      {
        ".js": "never",
        ".jsx": "never",
        ".ts": "never",
        ".tsx": "never"
      }
    ]
  }
}
